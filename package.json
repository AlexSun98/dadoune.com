{
  "name": "dadoune.com",
  "version": "1.2.0",
  "description": "Static site generator for dadoune.com",
  "homepage": "http://dadoune.com",
  "scripts": {
    "watch": "nodemon --watch build.js --watch scripts build.js",
    "build": "rm -rf build/ && NODE_ENV=production node build.js",
    "lint": "node_modules/eslint/bin/eslint.js --ignore-path .gitignore .",
    "depcheck": "depcheck --ignores=eslint,babelify,babel-preset-es2015,handlebars .",
    "test": "npm run depcheck && npm run lint"
  },
  "author": "Reed Dadoune <reed@dadoune.com>",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "__disqusShortName": true,
      "__siteVersion": true
    },
    "rules": {
      "quote-props": [
        "warn",
        "as-needed"
      ],
      "no-labels": "error",
      "comma-dangle": [
        "warn",
        "never"
      ],
      "no-empty-character-class": [
        "error"
      ],
      "object-curly-spacing": [
        "warn",
        "never"
      ],
      "semi-spacing": [
        "warn",
        {
          "before": false,
          "after": true
        }
      ],
      "keyword-spacing": [
        "warn",
        {
          "before": true,
          "after": true
        }
      ],
      "space-unary-ops": [
        "warn",
        {
          "words": true,
          "nonwords": false
        }
      ],
      "strict": [
        "error",
        "global"
      ],
      "no-confusing-arrow": [
        "error",
        {
          "allowParens": true
        }
      ],
      "no-console": "off",
      "no-unused-vars": [
        "error",
        {
          "args": "none"
        }
      ],
      "spaced-comment": [
        "warn",
        "always"
      ],
      "semi": 1,
      "space-before-function-paren": [
        "warn",
        "always"
      ]
    }
  },
  "dependencies": {
    "angular": "^1.6.1",
    "angular-disqus-comments": "^1.0.2",
    "angular-ui-router": "^0.4.2",
    "angulartics": "^1.3.0",
    "angulartics-google-analytics": "^0.4.0",
    "autoprefixer": "^6.7.0",
    "axios": "^0.15.3",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "bootstrap": "4.0.0-alpha.6",
    "browserify": "^14.0.0",
    "eslint": "^3.14.1",
    "handlebars": "^4.0.6",
    "helper-moment": "^0.2.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "metalsmith": "^2.3.0",
    "metalsmith-browser-sync": "^1.1.1",
    "metalsmith-browserify": "^0.2.4",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-drafts": "^0.0.1",
    "metalsmith-fingerprint-ignore": "^1.1.2",
    "metalsmith-html-minifier": "^2.2.0",
    "metalsmith-if": "^0.1.1",
    "metalsmith-imagemin": "^3.0.0",
    "metalsmith-layouts": "^1.7.0",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-move-up": "^1.0.0",
    "metalsmith-pagination": "^1.4.0",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-prism": "^3.0.2",
    "metalsmith-sass": "^1.4.0",
    "metalsmith-uglify": "^1.5.1",
    "minimatch": "^3.0.3",
    "multimatch": "^2.1.0",
    "nodemon": "^1.10.2",
    "postcss": "^5.2.11",
    "postcss-scss": "^0.4.0",
    "prismjs": "^1.6.0"
  },
  "devDependencies": {
    "bluebird": "^3.4.7",
    "nodegit": "^0.16.0",
    "nodegit-flow": "^0.3.0",
    "semver": "^5.3.0",
    "yargs": "^6.6.0"
  }
}
